$(document).ready(function(){var pathname=window.location.href.split('#')[0];$('a[href^="#"]').each(function(){var $this=$(this),link=$this.attr('href');$this.attr('href',pathname+link);});});$(window).scroll(function(){var scrollDistance=$(window).scrollTop();$('.sidebar-nav li').eq(0).addClass('active');$('.anchor').each(function(i){if($(this).position().top<=scrollDistance+100){$('.sidebar-nav li.active').removeClass('active');$('.sidebar-nav li').eq(i).addClass('active');}});}).scroll();if(typeof renderMathInElement==="function"){document.addEventListener("DOMContentLoaded",function(){renderMathInElement(document.body,{delimiters:[{left:"$$",right:"$$",display:true},{left:"\\[",right:"\\]",display:true},{left:"$",right:"$",display:false},{left:"\\(",right:"\\)",display:false}]});});}